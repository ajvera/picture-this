<div class="form-spacing">
	<legend>Register:</legend>
  <div class="form-group">
    <%= bootstrap_form_for(@user) do |f| %>
      <%= f.text_field :first_name %>
      <%= f.text_field :last_name %>
      <%= f.text_field :username %>
      <%= f.text_field :email %>
      <%= f.password_field :password %>
      <%= f.submit %>
    <% end %>

</div>
